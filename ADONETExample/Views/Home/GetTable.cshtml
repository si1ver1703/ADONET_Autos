            @Html.DisplayFor(modelItem => item.engine)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isKazakstan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.decs)
        </td>
        <td>
