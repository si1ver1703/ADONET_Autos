@model IEnumerable<ADONETExample.Models.Auto>

@{
    ViewBag.Title = "GetTable";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetTable</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
